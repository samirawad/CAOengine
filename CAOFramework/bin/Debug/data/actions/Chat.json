{
  "Selectors": {
    "SelectSmallMaleGroup": {
      "From": [ "AllAgents" ],
      "Groups": {
        "SmallMaleGroup": {
          "Filter": "IsMale",
          "How": "RndMulti",
          "Params": {
            "Min": 1,
            "Max": 3
          }
        }
      }
    },
    "SelectSmallFemaleGroup": {
      "From": [ "AllAgents" ],
      "Groups": {
        "SmallFemaleGroup": {
          "Filter": "IsFemale",
          "How": "RndMulti",
          "Params": {
            "Min": 1,
            "Max": 3
          }
        }
      }
    },
    "ChatGroup": {
      "From": [ "SelectSmallMaleGroup", "SelectSmallFemaleGroup" ],
      "Groups": {
        "Speaker": {
          "From": [ "SmallMaleGroup" ],
          "How": "RndOne"
        },
        "ChatTarget": {
          "From": [ "SmallFemaleGroup" ],
          "How": "RndOne"
        },
        "Audience": {
          "NotFrom": ["Speaker","ChatTarget"]
        }
      }
    }
  },
  "Filters": {
    "IsMale": {
      "Function": "HasValue",
      "Params": {
        "Property": "Data.Gender",
        "Value": "male"
      }
    },
    "IsFemale": {
      "Function": "HasValue",
      "Params": {
        "Property": "Data.Gender",
        "Value": "female"
      }
    }
  },
  "CompoundFilters": {

  },
  "Actions": {
    "DoChat": {
      "SetsStage": "DoChat",
      "Requires": "ChatGroup",
      "Description": "A chat event"
    }
  },
  "Outcomes": {
    "Flirt": {
      "ParentStages": [ "DoChat" ],
      "SetsStage": "NONE",
      "Selection": "ChatGroup",
      "Steps": [
        {
          "Function": "AddTag",
          "Parameters": {
            "Target": "ChatTarget",
            "Property": "Tags",
            "Value": "Complimented",
            "Description": "The target was chatted up."
          }
        },
        {
          "Function": "AddOccurence",
          "Parameters": {
            "Occurence": {
              "Description": "The pretty girl gets chatted up at a party.",
              "Actor": "Speaker",
              "Target": "ChatTarget",
              "Witness": "Audience",
              "ActorRole": {
                "Actions": [ "Flirt" ],
                "Upholds": [ "Confidence", "Hedonism" ],
                "Forsakes": [ "Celibacy" ]
              },
              "TargetRole": {
                "Actions": [ "Flirt" ],
                "Upholds": [ "Attractiveness", "Hedonism" ],
                "Forsakes": [ "Celibacy" ]
              }
            }
          }
        }
      ]
    }
  }
}
