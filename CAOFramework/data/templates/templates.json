{
  "Agent": {
    "Gender": "SelectGender",
    "Name": "RandomName",
    "Profession": "SelectProfession",
    "Traits": "SelectTraits"
  },
  "SelectGender": {
    "SelectOne": {
      "selection": [ "male", "female" ]
    }
  },
  "RandomName": {
    "GivenName": {
      "SwitchOnValues": {
        "switches": [
          {
            "path": "Gender",
            "value": "male",
            "return": "MaleFirstNameGenerator"
          },
          {
            "path": "Gender",
            "value": "female",
            "return": "FemaleFirstNameGenerator"
          }
        ]
      }
    },
    "SurName": "LastNameGenerator"
  },
  "MaleFirstNameGenerator": {
    "MarkovStr": {
      "order": 4,
      "minlength": 6,
      "datafile": "male.txt"
    }
  },
  "FemaleFirstNameGenerator": {
    "MarkovStr": {
      "order": 4,
      "minlength": 6,
      "datafile": "female.txt"
    }
  },
  "LastNameGenerator": {
    "MarkovStr": {
      "order": 2,
      "minlength": 8,
      "datafile": "allnames.txt"
    }
  },
  "SelectProfession": {
    "SubSelection": {
      "min": 1,
